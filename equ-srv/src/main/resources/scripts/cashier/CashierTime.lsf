MODULE CashierTime;

REQUIRE System, Integration, Employee, CashRegister;

CLASS CashierTime 'Время работы кассиров';
TABLE cashierTime (CashierTime);

@defineExternalizable(cashierTime, VARSTRING[100]);
employeeCashierTime 'Кассир' = DATA Employee (CashierTime);
shortNameContactCashierTime 'Кассир' (cashierTime) = shortNameContact(employeeCashierTime(cashierTime));
cashRegisterCashierTime 'Касса' = DATA CashRegister (CashierTime);
numberCashRegisterCashierTime 'Касса' (cashierTime) = nppMachinery(cashRegisterCashierTime(cashierTime));
logOnDateTimeCashierTime 'Вход в систему' = DATA DATETIME(CashierTime);
logOnDateCashierTime 'Вход в систему' = toDate(logOnDateTimeCashierTime(CashierTime));
logOnTimeCashierTime 'Вход в систему' = toTime(logOnDateTimeCashierTime(CashierTime));
logOffDateTimeCashierTime 'Выход из системы' = DATA DATETIME(CashierTime);
logOffDateCashierTime 'Выход из системы' = toDate(logOffDateTimeCashierTime(CashierTime));
logOffTimeCashierTime 'Выход из системы' = toTime(logOffDateTimeCashierTime(CashierTime));

FORM cashierTime 'Время работы кассиров'
    OBJECTS c = CashierTime
    PROPERTIES (c) shortNameContactCashierTime, numberCashRegisterCashierTime, logOnDateTimeCashierTime, logOffDateTimeCashierTime, ADDOBJ, DELETE
;

NAVIGATOR {
    machineryMasterData {
        ADD cashierTime;
    }
}