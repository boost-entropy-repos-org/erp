MODULE ImportBIVCDOS;

REQUIRE Import,
        PurchaseCertificateSOT,
        SOT;


//---------------------------------------------- Импорт данных -----------------------------------------------------//

GROUP bivcDosGroup 'bivcDos' : public;

importBIVCDOSDataAction 'Импортировать данные (BIVCDOS)' = ACTION CUSTOM 'integration.ImportBIVCDOSActionProperty' IN bivcDosGroup;
importBIVCDOSDirectory 'Папка' = DATA STRING[200] () IN bivcDosGroup;
importBIVCDOSNumberItems 'Кол-во' = DATA INTEGER () IN bivcDosGroup;
importBIVCDOSNumberItemsAtATime 'Разбивать на пакеты по' = DATA INTEGER () IN bivcDosGroup;

importBIVCDOSGroupItems 'Импортировать группы товаров' = DATA BOOLEAN () IN bivcDosGroup;
importBIVCDOSItems 'Импортировать товары' = DATA BOOLEAN () IN bivcDosGroup;
importBIVCDOSWares 'Импортировать посуду' = DATA BOOLEAN () IN bivcDosGroup;
importBIVCDOSUserInvoices 'Импортировать документы прихода' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSNumberUserInvoices 'Кол-во' = DATA INTEGER () IN bivcDosGroup;
importBIVCDOSNumberUserInvoicesAtATime 'Разбивать на пакеты по' = DATA INTEGER () IN bivcDosGroup;
importBIVCDOSUserInvoicesPosted 'Импортировать проведёнными' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSStartDate 'Начиная с даты' = DATA DATE () IN bivcDosGroup;
importBIVCDOSBanks 'Импортировать банки' = DATA BOOLEAN () IN bivcDosGroup;
importBIVCDOSLegalEntities 'Импортировать организации' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSEmployees 'Импортировать сотрудников' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSWarehouses 'Импортировать склады' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSMag2 'Импортировать коды MAG2' = DATA BOOLEAN() IN bivcDosGroup;
importBIVCDOSContracts 'Импортировать контракты' = DATA BOOLEAN() IN bivcDosGroup;

EXTEND FORM integrationData
    PROPERTIES() importBIVCDOSDataAction, importBIVCDOSDirectory, importBIVCDOSStartDate, importBIVCDOSNumberItems,
    importBIVCDOSNumberItemsAtATime, importBIVCDOSGroupItems, importBIVCDOSItems, importBIVCDOSWares, importBIVCDOSBanks,
    importBIVCDOSEmployees, importBIVCDOSLegalEntities, importBIVCDOSWarehouses, importBIVCDOSMag2, importBIVCDOSContracts,
    importBIVCDOSUserInvoices, importBIVCDOSUserInvoicesPosted, importBIVCDOSNumberUserInvoices,
    importBIVCDOSNumberUserInvoicesAtATime
;
EXTEND DESIGN integrationData {
    pane {
        NEW bivcDos {
            title = 'BIVCDOS';
            ADD NOGROUP.bivcDosGroup;
        }
    }
}
