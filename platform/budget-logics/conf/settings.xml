<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
       default-lazy-init="true">

    <context:property-placeholder location="file:conf/settings.properties"/>

    <import resource="classpath:settings/common-bl-settings.xml"/>

    <bean id="businessLogics" class="budget.BudgetBusinessLogics">
        <property name="overridingModulesList" value="${logics.overridingModulesList}"/>
    </bean>
</beans>
