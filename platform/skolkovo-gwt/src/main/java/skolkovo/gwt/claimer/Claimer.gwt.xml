<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="platform.gwt.sgwtbase.SGWTBase"/>
    <inherits name="skolkovo.api.gwt.API"/>
    <inherits name="skolkovo.gwt.Resources"/>

    <!-- Include GWTUpload library -->
    <inherits name="gwtupload.GWTUpload"/>
    <!-- Load dinamically predefined styles in the library when the application starts -->
    <stylesheet src="Upload.css"/>

    <entry-point class='skolkovo.gwt.claimer.client.ClaimerFrame'/>

    <source path='client'/>
    <source path='shared'/>
</module>