MODULE PriceListLedgerItem;

REQUIRE PriceListLedger, Item;

NAMESPACE PriceList;

showPriceListLedgerStock 'Показать изменение цены товара по складу' (Sku sku) = ACTION SHOW movementPriceSkuStock OBJECTS s = sku  SHORTCUT nameAttribute[Item];
showPriceListLedger 'Показать изменение цены товара' (Sku sku) = ACTION SHOW movementPriceSkuStocks OBJECTS s = sku  SHORTCUT nameAttribute[Item];
