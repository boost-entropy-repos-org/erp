MODULE ItemStorage;

REQUIRE System, Item;

NAMESPACE Item;

expiryTextItem 'Срок хранения' = DATA  VARISTRING[100] (Item) IN itemExpiry;
conditionsTextItem 'Условия хранения' = DATA  VARISTRING[100] (Item) IN itemExpiry;

EXTEND FORM item PROPERTIES(i) SHOWIF showExpiryItem(i) expiryTextItem, conditionsTextItem;