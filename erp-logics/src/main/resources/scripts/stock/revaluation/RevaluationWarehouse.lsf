MODULE RevaluationWarehouse;

REQUIRE Revaluation, Warehouse;

NAMESPACE Revaluation;

EXTEND FORM warehouse
     PROPERTIES(w) SHOWIF isCompany(w) nameRepricingCommittee, numberDisposal
;

DESIGN warehouse {
    headerRight {
        MOVE PROPERTY(numberDisposal(w));    
        MOVE w.committee {
            columns = 1;
        }
    }
}