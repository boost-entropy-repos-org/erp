MODULE RevaluationWarehouse;

REQUIRE Revaluation, Warehouse;

NAMESPACE Revaluation;

EXTEND FORM warehouse
     PROPERTIES(w) SHOWIF isCompanyStock(w) nameRepricingCommitteeStock, numberDisposalStock
;

DESIGN warehouse {
    headerRight {
        ADD PROPERTY(numberDisposalStock(w));    
        ADD w.committee {
            columns = 1;
        }
    }
}