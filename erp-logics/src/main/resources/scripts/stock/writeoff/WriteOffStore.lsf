MODULE WriteOffStore;
   
   REQUIRE WriteOff, Store;
   
   NAMESPACE WriteOff;
   
   EXTEND FORM departmentStore
       PROPERTIES(d) SHOWIF isCompanyStock(d) nameWriteOffCommitteeStock
   ;
   
   EXTEND DESIGN departmentStore {
       headerRight {
           ADD d.committee {
               columns = 1;
           }
       }
   }