MODULE SkuImportCode;

REQUIRE Item, ImportCode;

NAMESPACE Stock;

item = DATA Item (ImportCode);

@defineExternalizable(importCode, VARSTRING[100], item);

EXTEND FORM item
    OBJECTS li = ImportCode
    PROPERTIES(li) itemId, NEW, DELETE GRID
    
    FILTERS item(li)==i
;

DESIGN item {
    tabContaner{
        MOVE BOX(li);
    }
}

replace(Item s, Item d) += {item(ImportCode code) <- d WHERE item(code) == s;}

@defineImportCode(Manufacturer, attributesItem, im, manufacturer);

@defineImportCode(Country, attributesItem, ic, country);

@defineImportCode(UOM, attributesItem, iu, UOM);