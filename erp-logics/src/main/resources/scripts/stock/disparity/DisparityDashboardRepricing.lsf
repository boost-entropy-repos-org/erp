MODULE DisparityDashboardRepricing;

REQUIRE DisparityDashboard, RepricingLabel, RepricingDisparity, RepricingMachinery;

NAMESPACE Disparity;

createRepricingDisparityLabelTransaction 'Распечатать ценники' = ACTION createRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createSnapshotRepricingDisparityLabelTransaction 'Перепечатать ценники' = ACTION createSnapshotRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createRepricingDisparityMachineryPriceTransaction 'Загрузить в оборудование' = ACTION createRepricingMachineryPriceTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;

EXTEND FORM disparityItemItemDashboard
    PROPERTIES (dis) createRepricingDisparityLabelTransaction, createSnapshotRepricingDisparityLabelTransaction, createRepricingDisparityMachineryPriceTransaction
;    