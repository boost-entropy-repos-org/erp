MODULE DisparityDashboardRepricing;

REQUIRE DisparityDashboard, RepricingLabel, RepricingDisparity, RepricingMachinery;

NAMESPACE Disparity;

createRepricingDisparityLabelTransaction 'Распечатать ценники' = createRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createSnapshotRepricingDisparityLabelTransaction 'Перепечатать ценники' = createSnapshotRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createRepricingDisparityMachineryPriceTransaction 'Загрузить в оборудование' = createRepricingMachineryPriceTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;

EXTEND FORM disparityItemItemDashboard
    PROPERTIES (dis) createRepricingDisparityLabelTransaction, createSnapshotRepricingDisparityLabelTransaction, createRepricingDisparityMachineryPriceTransaction
;    