MODULE DisparityDashboardRepricing;

REQUIRE DisparityDashboard, RepricingLabel, RepricingDisparity, RepricingMachinery;

NAMESPACE Disparity;

createRepricingDisparityLabelTransaction 'Распечатать ценники' (disparity) = ACTION createRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createSnapshotRepricingDisparityLabelTransaction 'Перепечатать ценники' (disparity) = ACTION createSnapshotRepricingLabelTransaction (disparityRepricingDisparity(disparity)) TOOLBAR;
createRepricingDisparityMachineryPriceTransaction 'Загрузить в оборудование' (disparity) = ACTION createMachineryPriceTransactionRepricing (disparityRepricingDisparity(disparity)) TOOLBAR;

EXTEND FORM disparityItemItemDashboard
    PROPERTIES (dis) createRepricingDisparityLabelTransaction, createSnapshotRepricingDisparityLabelTransaction, createRepricingDisparityMachineryPriceTransaction
;    