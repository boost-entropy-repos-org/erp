MODULE PurchaseScheduleOrderReserve;

REQUIRE PurchaseScheduleOrder, PurchaseReserve;

NAMESPACE Purchase;

overCreateOrder(userOrder) += ACTION {
    executeLocalEvents();
    fillRecommendedQuantityStockUserOrder(userOrder);
};

overCreateOrder(o) += ACTION {
    dateFromSoldPeriodUserOrder(o) <-  fromShipmentDateUserOrder(o);   
}
