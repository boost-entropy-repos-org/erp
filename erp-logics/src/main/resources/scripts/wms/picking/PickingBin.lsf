MODULE PickingBin;

REQUIRE Picking, Bin, PickingOrderBin;

NAMESPACE Picking;

@defineDocumentInterfaceDetailBin(picking);

EXTEND FORM userPicking
    PROPERTIES(d) nameBin
;

EXTEND FORM pickings
    PROPERTIES(d) READONLY nameBin
;

EXTEND FORM pickingsOrderPickings
    PROPERTIES(d) READONLY nameBin
;

EXTEND FORM printPickingOrder
    PROPERTIES(d) READONLY nameBin
;

overCopy (UserPickingDetail d, PickingDetail s) += ACTION {
    bin(d) <- bin(s);
}

overFillUserPicking (UserPickingDetail d, PickingOrderDetail s) += ACTION {
    bin(d) <- bin(s);
}