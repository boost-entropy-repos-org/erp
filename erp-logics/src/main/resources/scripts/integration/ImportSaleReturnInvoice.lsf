MODULE ImportSaleReturnInvoice;

REQUIRE System,
        ImportDocument,
        Item,
        SaleReturnInvoice;

NAMESPACE SaleReturn; 


original 'Файл оригинала накладной'  = DATA CUSTOMFILE (Invoice);

saveOriginal 'Загрузить файл оригинала накладной' (Invoice i) = { LOADFILE original(i); }
openOriginal 'Просмотреть оригинал накладной' (Invoice i) = { OPENFILE original(i); }

EXTEND FORM userInvoice
    PROPERTIES(i) saveOriginal, openOriginal 
;

DESIGN userInvoice{
    import {
        caption = 'Импорт';
        type = CONTAINERH;
        NEW origin {
            caption = 'Оригинал накладной';
            MOVE PROPERTY(saveOriginal(i));
            MOVE PROPERTY(openOriginal(i));
        }
    }
}




