MODULE POSInvoicePayment;

REQUIRE POS, Invoice, Payment;

fiscalTypePayment 'Тип платежа' = ABSTRACT INTEGER (Payment.Payment); 
EXTEND FORM payment PROPERTIES(p) fiscalTypePayment;
EXTEND DESIGN payment {
    paramContainer {
        ADD PROPERTY(fiscalTypePayment(p));
    }
}
printInvoicePayment 'Напечатать оплату' = ABSTRACT ACTION LIST (Invoice.Invoice, Payment.Payment);