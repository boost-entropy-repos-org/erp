MODULE RepricingStore;

REQUIRE Repricing, Store;

NAMESPACE Repricing;

EXTEND FORM departmentStore
    PROPERTIES(d) SHOWIF isCompanyStock(d) nameRepricingCommitteeStock, numberDisposalStock
;

DESIGN departmentStore {
    headerRight {
        ADD PROPERTY(numberDisposalStock(d));
        ADD d.committee {
            columns = 1;
        }
    }
}