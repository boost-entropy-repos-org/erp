MODULE RepricingStore;

REQUIRE Repricing, Store;

NAMESPACE Repricing;

EXTEND FORM departmentStore
    PROPERTIES(d) SHOWIF isCompany(d) nameRepricingCommittee, numberDisposal
;

DESIGN departmentStore {
    headerRight {
        MOVE PROPERTY(numberDisposal(d));
        MOVE d.committee {
            columns = 1;
        }
    }
}