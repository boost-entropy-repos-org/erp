MODULE StoreDefault;

REQUIRE Store, StockDefault;

NAMESPACE Store;

loadDefaultStockGroups () += {
    NEW c = ChainStores {
        name(c) <- 'Моя торговая сеть';
        id(c) <- 'chainStores';
        NEW s = StoreType {
            name(s) <- 'Мой формат';
            id(s) <- 'storeType';
            chainStores(s) <- c;
        }
    }
}