MODULE ZReportTax;

REQUIRE SkuLedgerTax, ZReport;

NAMESPACE Stock;

overValueVATReceiptDetail(d) += valueVATBatch(batchReceiptDetail(d));

WHEN SESSION SETCHANGED(batchReceiptSaleDetail(d)) DO {
    VATReceiptSaleDetail(d) <- VATBatch(batchReceiptSaleDetail(d));
}
WHEN SESSION SETCHANGED(batchReceiptReturnDetail(d)) DO {
    VATReceiptReturnDetail(d) <- VATBatch(batchReceiptReturnDetail(d));
}                                