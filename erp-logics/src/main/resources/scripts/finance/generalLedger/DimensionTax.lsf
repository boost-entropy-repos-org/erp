MODULE DimensionTax;

REQUIRE Dimension, Tax;

EXTEND CLASS Range : Dimension;
EXTEND CLASS DimensionType {
    range 'Шкала'
}
nameDimension (dimension) += staticCaption(taxRange(dimension)) + ', шкала № ' + numberRange(dimension) + ', тек.ставка (' + valueCurrentRateRange(dimension) + ')';
dimensionTypeDimension (dimension) += WHEN dimension IS Range THEN DimensionType.range;

idDimension(dimension) += VARISTRING[20](numberRange(dimension));
