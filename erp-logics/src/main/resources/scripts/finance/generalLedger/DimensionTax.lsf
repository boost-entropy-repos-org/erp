MODULE DimensionTax;

REQUIRE Dimension, Tax;

EXTEND CLASS Range : Dimension;
EXTEND CLASS DimensionType {
    range 'Шкала'
}
nameDimension (dimension) += [= FORMULA VARSTRING[255]  ' CAST($1 AS TEXT) || \', шкала № \' || CAST($2 AS TEXT) || \', тек.ставка\' || \' ( \' || CAST($3 AS TEXT)|| \')\''](
    staticCaption(taxRange(dimension)), numberRange(dimension), valueCurrentRateRange(dimension));
dimensionTypeDimension (dimension) += WHEN dimension IS Range THEN DimensionType.range;

idDimension(dimension) += VARISTRING[20](numberRange(dimension));
