MODULE DimensionCashRegister;

REQUIRE Dimension, CashRegister;

//-- Employee
EXTEND CLASS CashRegister : Dimension;
EXTEND CLASS DimensionType {
    cashRegister 'Касса'
}
name (CashRegister dimension) += VARISTRING[255](shortDescription(dimension)) IF dimension IS CashRegister;
dimensionType (CashRegister dimension) += WHEN dimension IS CashRegister THEN DimensionType.cashRegister;

id(CashRegister dimension) += VARISTRING[100](npp[Machinery](dimension)) IF dimension IS CashRegister;
