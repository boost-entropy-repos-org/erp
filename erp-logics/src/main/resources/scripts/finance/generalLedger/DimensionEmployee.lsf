MODULE DimensionEmployee;

REQUIRE Dimension, Employee;

//-- Employee
EXTEND CLASS Employee : Dimension;
EXTEND CLASS DimensionType {
    employee 'МОЛ (сотрудник)'
}
nameDimension (dimension) += VARISTRING[255](positionNameEmployee(dimension));
dimensionTypeDimension (dimension) += WHEN dimension IS Employee THEN DimensionType.employee;

idDimension(dimension) += idEmployee(dimension);
