MODULE DimensionContract;

REQUIRE Dimension, Contract;

EXTEND CLASS Contract : Dimension;
EXTEND CLASS DimensionType {
    contract 'Договор'
}
nameDimension (dimension) += descriptionContract(dimension) IF dimension IS Contract;
dimensionTypeDimension (dimension) += WHEN dimension IS Contract THEN DimensionType.contract;

idDimension(dimension) += VARISTRING[100](OVERRIDE seriesNumberContract(dimension), idContract(dimension)) IF dimension IS Contract;
