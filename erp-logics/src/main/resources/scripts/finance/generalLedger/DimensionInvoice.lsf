MODULE DimensionInvoice;

REQUIRE Dimension, Invoice;

EXTEND CLASS Invoice : Dimension;
EXTEND CLASS DimensionType {
    invoice 'Накладная'
}
nameDimension (dimension) += descriptionInvoice(dimension) IF dimension IS Invoice;
dimensionTypeDimension (dimension) += WHEN dimension IS Invoice THEN DimensionType.invoice;

idDimension(dimension) += VARISTRING[100](seriesNumberInvoice(dimension));