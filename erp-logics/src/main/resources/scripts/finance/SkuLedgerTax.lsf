MODULE SkuLedgerTax;

REQUIRE SkuLedger, TaxItem;

NAMESPACE Stock;

valueVATBatchDefaultCountryCurrentDate 'НДС, %' (batch) = valueRateRangeDate(VATItemCountryDate(skuBatch(batch), defaultCountry(), currentDate()), currentDate());

EXTEND FORM batches 
    PROPERTIES (bt) READONLY valueVATBatchDefaultCountryCurrentDate AFTER nameStockBatch(bt)
;    