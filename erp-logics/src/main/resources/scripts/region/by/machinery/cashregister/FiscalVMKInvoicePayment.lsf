MODULE FiscalVMKInvoicePayment;

REQUIRE FiscalVMK, POSInvoice;

fiscalVMKPrintInvoicePayment 'Напечатать оплату' = ACTION CUSTOM 'lsfusion.erp.region.by.machinery.cashregister.fiscalvmk.FiscalVMKPrintInvoicePaymentActionProperty' (Sale.Invoice, Payment.Payment);
printReceiptInvoicePayment(invoice, payment) += ACTION IF sidModelCurrentCashRegister() == 'VMK' THEN fiscalVMKPrintInvoicePayment(invoice, payment);

