MODULE FiscalAbsolutInvoicePayment;

REQUIRE FiscalAbsolut, POSInvoice;

fiscalAbsolutPrint 'Напечатать оплату' = ACTION CUSTOM 'lsfusion.erp.region.by.machinery.cashregister.fiscalabsolut.FiscalAbsolutPrintInvoicePaymentActionProperty' (Sale.Invoice, Payment.Payment);
printReceipt(Sale.Invoice invoice, Payment.Payment payment) += ACTION IF sidModelCurrentCashRegister() == 'Absolut' THEN fiscalAbsolutPrint(invoice, payment);

fiscalAbsolutPrintReturn 'Напечатать возврат' = ACTION CUSTOM 'lsfusion.erp.region.by.machinery.cashregister.fiscalabsolut.FiscalAbsolutPrintReturnInvoicePaymentActionProperty' (Sale.Invoice, Payment.Payment);
printReturnReceipt(Sale.Invoice invoice, Payment.Payment payment) += ACTION IF sidModelCurrentCashRegister() == 'Absolut' THEN fiscalAbsolutPrintReturn(invoice, payment);
