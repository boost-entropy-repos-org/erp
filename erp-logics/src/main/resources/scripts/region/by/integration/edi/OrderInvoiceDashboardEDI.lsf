MODULE OrderInvoiceDashboardEDI;

REQUIRE OrderInvoiceDashboard, EDI;

NAMESPACE EDI;


despatchAdvices 'Уведомления об отгрузке' = GROUP BY eOrder(EOrderDespatchAdvice a) CONCAT deliveryNoteNumber(a) , ',' CHARWIDTH 10;
 
EXTEND FORM orderInvoiceDashboard
    PROPERTIES (o) READONLY despatchAdvices BEFORE note(o)
    FILTERGROUP despatch 
        FILTER 'Уведомления об отгрузке' despatchAdvices(o); 