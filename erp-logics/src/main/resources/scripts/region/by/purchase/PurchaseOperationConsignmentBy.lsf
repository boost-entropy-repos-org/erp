MODULE PurchaseOperationConsignmentBy;

REQUIRE PurchaseOperation, OperationConsignmentBy;

NAMESPACE Purchase;

@defineOperationConsignment();

isInvoicePriceConsignmentOperation 'Печатать в качестве цены цену с НДС' = DATA BOOLEAN (Operation);

EXTEND FORM operation
    PROPERTIES isInvoicePriceConsignmentOperation(o)
;

DESIGN operation {
    consignment {
        MOVE PROPERTY(isInvoicePriceConsignmentOperation(o));
    }
}
