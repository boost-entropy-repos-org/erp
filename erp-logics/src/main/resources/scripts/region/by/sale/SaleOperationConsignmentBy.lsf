MODULE SaleOperationConsignmentBy;

REQUIRE SaleOperation, OperationConsignmentBy;

NAMESPACE Sale;

@defineOperationConsignment();
isInvoicePriceConsignment 'Печатать в качестве цены цену с НДС' = DATA BOOLEAN (Operation);

EXTEND FORM operation
    PROPERTIES isInvoicePriceConsignment(o)
;

DESIGN operation {
    consignment {
        MOVE PROPERTY(isInvoicePriceConsignment(o));
    }
}