MODULE StoreConsignmentBy;

REQUIRE ConsignmentBy, Store; 

NAMESPACE Consignment;
                                        
EXTEND FORM departmentStore
    PROPERTIES(d) SHOWIF isCompanyStock(d) nameIssuanceAllowedStock, nameIssuanceExecutedStock, nameLoadingExecuterStock, 
                  nameWayOfLoadingStock, nameUnloadingExecuterStock, nameWayOfUnloadingStock, codeLoadingStock,
                  nameConsigmentLegalEntityStock, addressConsigmentLegalEntityStock
;

DESIGN departmentStore {
    headerLeft {
        MOVE d.invoiceConsignment AFTER headerExtraParams {
            columns = 3;
            MOVE PROPERTY(nameIssuanceAllowedStock(d));
            MOVE PROPERTY(nameIssuanceExecutedStock(d));
            MOVE PROPERTY(nameLoadingExecuterStock(d));
            MOVE PROPERTY(nameWayOfLoadingStock(d));
            MOVE PROPERTY(nameUnloadingExecuterStock(d));
            MOVE PROPERTY(nameWayOfUnloadingStock(d));
            MOVE PROPERTY(codeLoadingStock(d));
            MOVE PROPERTY(nameConsigmentLegalEntityStock(d));
            MOVE PROPERTY(addressConsigmentLegalEntityStock(d));
        }
    }
}
