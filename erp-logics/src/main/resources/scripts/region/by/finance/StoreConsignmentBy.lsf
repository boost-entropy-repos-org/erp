MODULE StoreConsignmentBy;

REQUIRE ConsignmentBy, Store; 

NAMESPACE Consignment;
                                        
EXTEND FORM departmentStore
    PROPERTIES(d) nameIssuanceAllowedStock, nameIssuanceExecutedStock, nameLoadingExecuterStock, 
                  nameWayOfLoadingStock, codeLoadingStock
;

EXTEND DESIGN departmentStore {
    headerLeft {
        ADD d. invoiceConsignment AFTER headerExtraParams {
            columns = 3;
            ADD PROPERTY(nameIssuanceAllowedStock);
            ADD PROPERTY(nameIssuanceExecutedStock);
            ADD PROPERTY(nameLoadingExecuterStock);
            ADD PROPERTY(nameWayOfLoadingStock);
            ADD PROPERTY(codeLoadingStock);
        }
    }
}
