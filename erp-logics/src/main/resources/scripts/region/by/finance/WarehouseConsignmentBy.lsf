MODULE WarehouseConsignmentBy;

REQUIRE ConsignmentBy, Warehouse; 

NAMESPACE Consignment;
                                        
EXTEND FORM warehouse
    PROPERTIES(w) SHOWIF isCompanyStock(w) nameIssuanceAllowedStock, nameIssuanceExecutedStock, nameLoadingExecuterStock, 
                  nameWayOfLoadingStock, nameUnloadingExecuterStock, nameWayOfUnloadingStock, codeLoadingStock
    PROPERTIES(w) nameConsigmentLegalEntityStock, addressConsigmentLegalEntityStock                  
;

EXTEND DESIGN warehouse {
    headerLeft {
        ADD w.invoiceConsignment AFTER headerExtraParams {
            columns = 3;
            ADD PROPERTY(nameIssuanceAllowedStock(w));
            ADD PROPERTY(nameIssuanceExecutedStock(w));
            ADD PROPERTY(nameLoadingExecuterStock(w));
            ADD PROPERTY(nameWayOfLoadingStock(w));
            ADD PROPERTY(nameUnloadingExecuterStock(w));
            ADD PROPERTY(nameWayOfUnloadingStock(w));
            ADD PROPERTY(codeLoadingStock(w));
            ADD PROPERTY(nameConsigmentLegalEntityStock(w));
            ADD PROPERTY(addressConsigmentLegalEntityStock(w));
        }
    }
}
