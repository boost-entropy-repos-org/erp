MODULE WarehouseConsignmentBy;

REQUIRE ConsignmentBy, Warehouse; 

NAMESPACE Consignment;
                                        
EXTEND FORM warehouse
    PROPERTIES(w) SHOWIF isCompanyStock(w) nameIssuanceAllowedStock, nameIssuanceExecutedStock, nameLoadingExecuterStock, 
                  nameWayOfLoadingStock, nameUnloadingExecuterStock, nameWayOfUnloadingStock, codeLoadingStock
    PROPERTIES(w) nameConsigmentLegalEntityStock, addressConsigmentLegalEntityStock                  
;

DESIGN warehouse {
    headerLeft {
        MOVE w.invoiceConsignment AFTER headerExtraParams {
            columns = 3;
            MOVE PROPERTY(nameIssuanceAllowedStock(w));
            MOVE PROPERTY(nameIssuanceExecutedStock(w));
            MOVE PROPERTY(nameLoadingExecuterStock(w));
            MOVE PROPERTY(nameWayOfLoadingStock(w));
            MOVE PROPERTY(nameUnloadingExecuterStock(w));
            MOVE PROPERTY(nameWayOfUnloadingStock(w));
            MOVE PROPERTY(codeLoadingStock(w));
            MOVE PROPERTY(nameConsigmentLegalEntityStock(w));
            MOVE PROPERTY(addressConsigmentLegalEntityStock(w));
        }
    }
}
