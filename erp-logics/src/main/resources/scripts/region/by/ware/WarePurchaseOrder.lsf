MODULE WarePurchaseOrder;

REQUIRE WareOrder, PurchaseOrder;

NAMESPACE Purchase;

@defineOrderWare(Purchase);

prevListSku(PriceListType t, UserOrderDetail d) += WHEN itemWareUserOrderDetail(d) AND t == priceListType(d) AND invoicePrice(d) THEN NUMERIC[14,2](round2(invoicePrice(d)*100/(100+calcValueVAT(d))));
