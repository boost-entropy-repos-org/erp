MODULE WarePurchaseOrder;

REQUIRE WareOrder, PurchaseOrder;

NAMESPACE Purchase;

@defineOrderWare(Purchase);

prevList(PriceListType t, UserOrderDetail d) += WHEN t == priceListType(d) AND itemWareUserOrderDetail(d) AND invoicePrice(d) THEN NUMERIC[14,2](round2(invoicePrice(d)*100/(100+calcValueVAT(d))));
