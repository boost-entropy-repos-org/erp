PRIORITY PurchaseReturn;

//----------------------------------------------------------------------------//

overPricingPriceUserInvoiceDetail(detail) += manufacturingPriceUserInvoiceDetail(detail) IF NOT skipCalcPricingManufacturingPrice();

SaleReturn.showManufacturingPriceInvoice(invoice) += showManufacturingPriceInvoice(invoiceSaleReturnInvoice(invoice));
SaleReturn.manufacturingPriceInvoiceDetail(invoiceDetail) += manufacturingPriceInvoiceDetail(invoiceDetailSaleReturnInvoiceDetail(invoiceDetail));
SaleReturn.manufacturingSumInvoiceDetail(invoiceDetail) += manufacturingSumInvoiceDetail(invoiceDetailSaleReturnInvoiceDetail(invoiceDetail));

overFillInvoiceUserInvoiceInvoice (sd, id) += ACTION ASSIGN SaleReturn.showManufacturingPriceUserInvoice (sd) <- showManufacturingPriceInvoice(id);
overFillInvoiceUserInvoiceDetailInvoiceDetail (sd, id) += ACTION ASSIGN SaleReturn.manufacturingPriceUserInvoiceDetail (sd) <- manufacturingPriceInvoiceDetail(id);
