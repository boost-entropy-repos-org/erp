MODULE DisparityPharmacy;

REQUIRE Disparity, StockPharmacy;

NAMESPACE Disparity;

importCountryDispariryDetail = DATA Country (DisparityDetail);

importCountryDispariryDetail(detail) <- importCountryBatch(batchDisparityDetail(detail)) WHEN CHANGED(batchDisparityDetail(detail));
importCountryBatch(batch) += PREV(importCountryDispariryDetail(disparityDetailInputDisparityDetail(batch)));

seriesPharmacyDispariryDetail = DATA VARSTRING[20] (DisparityDetail);

seriesPharmacyDispariryDetail(detail) <- prevSeriesPharmacyBatch(batchDisparityDetail(detail)) WHEN CHANGED(batchDisparityDetail(detail));
seriesPharmacyBatch(batch) += seriesPharmacyDispariryDetail(disparityDetailInputDisparityDetail(batch));

contractPriceDispariryDetail = DATA VARSTRING[20] (DisparityDetail);

contractPriceDispariryDetail(detail) <- prevContractPriceBatch(batchDisparityDetail(detail)) WHEN CHANGED(batchDisparityDetail(detail));
contractPriceBatch(batch) += contractPriceDispariryDetail(disparityDetailInputDisparityDetail(batch));


    
    

    
