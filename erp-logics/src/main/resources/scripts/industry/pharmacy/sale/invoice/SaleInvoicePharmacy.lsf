MODULE SaleInvoicePharmacy;

REQUIRE SaleInvoice, StockPharmacy;

NAMESPACE Sale;

seriesPharmacyInvoiceDetail 'Серия лекарственного средства' = seriesPharmacyBatch(batchInvoiceDetail(detail));
seriesPharmacyUserInvoiceDetail 'Серия лекарственного средства' = seriesPharmacyBatch(batchUserInvoiceDetail(detail));

EXTEND FORM userInvoice
    PROPERTIES(d) READONLY seriesPharmacyUserInvoiceDetail SHOWIF showSeriesPharmacy() AFTER nameBatchUserInvoiceDetail(d)
;

EXTEND FORM invoices
    PROPERTIES(d) READONLY seriesPharmacyInvoiceDetail SHOWIF showSeriesPharmacy() AFTER nameBatchInvoiceDetail(d)
;
