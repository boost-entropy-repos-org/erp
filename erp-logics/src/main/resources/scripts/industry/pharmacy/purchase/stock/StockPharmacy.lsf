MODULE StockPharmacy;

REQUIRE Stock, PurchaseShipmentPharmacy;

NAMESPACE Stock;

seriesPharmacyBatch 'Серия лекарственного средства' = ABSTRACT VARSTRING[100] (Batch) PERSISTENT;

seriesPharmacyBatch (batch) += seriesPharmacyShipmentDetail(shipmentDetailShipmentBatch(batch));

EXTEND FORM currentBalanceBatchStock
    PROPERTIES READONLY seriesPharmacyBatch(bt) SHOWIF showSeriesPharmacy() AFTER nameSkuBatch(bt)
;

EXTEND FORM dialogBatchStock
    PROPERTIES READONLY seriesPharmacyBatch(bt) SHOWIF showSeriesPharmacy() AFTER nameSkuBatch(bt)
;

EXTEND FORM batches
    PROPERTIES READONLY seriesPharmacyBatch(bt) SHOWIF showSeriesPharmacy() AFTER nameSkuBatch(bt)
;


