MODULE NewLandBoard;

REQUIRE System, PriceChecker;

hostNewLandBoard 'Хост' () = DATA VARSTRING[100] (); 
portNewLandBoard 'Порт' () = DATA INTEGER ();        
        
EXTEND FORM options PROPERTIES() hostNewLandBoard, portNewLandBoard;

DESIGN options {
    machinery {
        NEW newland {
            caption = 'Настройки прайс-чекера NewLand';
            MOVE PROPERTY (hostNewLandBoard());
            MOVE PROPERTY (portNewLandBoard());
        }
    }
}