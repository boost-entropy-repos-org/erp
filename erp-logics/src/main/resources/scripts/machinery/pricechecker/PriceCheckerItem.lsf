MODULE PriceCheckerItem;

REQUIRE Item, PriceCheckerPriceTransaction;

overTitlePriceCheckerMachineryPriceTransactionBarcode = ABSTRACT VARISTRING[255] (MachineryPriceTransaction, Barcode);

createAttributeMachineryPriceTransactionGroupMachinery(transaction, groupMachinery) += ACTION (transaction, groupMachinery) {
    IF groupMachinery IS GroupPriceChecker THEN
        nameMachineryPriceTransactionBarcode(transaction, barcode) <- (OVERRIDE titleCashRegisterBarcode(barcode) IF transaction IS MachineryPriceTransaction, overTitlePriceCheckerMachineryPriceTransactionBarcode(transaction, barcode)) WHERE inMachineryPriceTransactionBarcode(transaction, barcode);
}