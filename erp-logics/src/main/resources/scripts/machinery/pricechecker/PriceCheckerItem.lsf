MODULE PriceCheckerItem;

REQUIRE Item, PriceCheckerPriceTransaction;

overTitlePriceCheckerBarcode = ABSTRACT VARSTRING[255] (Barcode);

createAttributeMachineryPriceTransactionGroupMachinery(transaction, groupMachinery) += ACTION (transaction, groupMachinery) {
    IF groupMachinery IS GroupPriceChecker THEN
        nameMachineryPriceTransactionBarcode(transaction, barcode) <- (OVERRIDE titleCashRegisterBarcode(barcode), overTitlePriceCheckerBarcode(barcode)) WHERE inMachineryPriceTransactionBarcode(transaction, barcode);
}