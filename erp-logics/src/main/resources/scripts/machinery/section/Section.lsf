MODULE Section;

REQUIRE Sku, CashRegister;

CLASS Section 'Секция';
TABLE Section (Section);

@defineExternalizable(section, VARSTRING[100]);

nameSection 'Наименование' = DATA VARSTRING[255](Section);

dataInSectionSku 'Отм' = ABSTRACT BOOLEAN (Section, Sku);


FORM sections 'Секции' 
    OBJECTS s = Section
    PROPERTIES(s) idSection, nameSection, ADDOBJ FORCE PANEL TOOLBAR, DELETE FORCE PANEL TOOLBAR
;

NAVIGATOR {
    cashRegisterNavigator {
        ADD sections;
    }
}