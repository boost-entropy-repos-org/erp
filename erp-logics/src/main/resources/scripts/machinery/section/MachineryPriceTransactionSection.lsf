MODULE MachineryPriceTransactionSection;

REQUIRE MachineryPriceTransaction, Section;

sectionBarcode 'Секция' = OVERRIDE idSection(defaultSection()), [=GROUP CONCAT idSection(section) IF dataInSectionSku(section,skuBarcode(barcode)), ',' BY barcode](barcode);

@defineMachineryPriceTransactionAttribute(section, 'Код секции', VARSTRING[100]);

deleteSectionBarcode 'Секция (удаление)' = GROUP CONCAT idSection(section) IF NOT dataInSectionSku(section,skuBarcode(barcode)), ',' BY barcode;