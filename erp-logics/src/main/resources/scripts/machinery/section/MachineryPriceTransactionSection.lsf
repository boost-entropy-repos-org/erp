MODULE MachineryPriceTransactionSection;

REQUIRE MachineryPriceTransaction, Section;

section 'Секция' = OVERRIDE id(defaultSection()), [=GROUP CONCAT id(Section section) IF dataIn(section,sku(Barcode barcode)) AND NOT inactive(section), ',' BY barcode](Barcode barcode);

@defineMachineryPriceTransactionAttribute(section, 'Код секции', VARSTRING[100]);

deleteSectionBarcode 'Секция (удаление)' = GROUP CONCAT id(Section section) IF NOT dataIn(section,sku[Barcode](barcode)) AND NOT inactive(section), ',' BY barcode;