NAMESPACE Machinery;

dataMinPrice(Barcode b, Stock st) += (IF sid(country(sku(b))) == '112' THEN minAlcoholLimitPriceRB(sku(b), currentDateTime()) ELSE minAlcoholLimitPrice(sku(b), currentDateTime())) IF st IS Stock;
