loadDefaultTerminalModel 'Добавить модель терминала'(STRING[20] sid, ISTRING[110] name, STRING[200] handler)  { 
    IF NOT terminalSID(sid) THEN {
        NEW tm = TerminalModel {
             sid(tm) <- sid;   
             name(tm) <- name;
             handler(tm) <- handler;
        }
    }
}

