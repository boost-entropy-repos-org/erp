markupSumSold (LegalEntity le, INTEGER w, INTEGER y) = GROUP SUM markupSumSold(Stock s, le, SkuGroup g, DATE d) IF access(s) AND (filter(g) OR allGroups()) BY extractWeek(d), extractYear(d);
sumSold (LegalEntity le, INTEGER w, INTEGER y) = GROUP SUM sumSold(Stock s, le, SkuGroup g, DATE d) IF access(s) AND (filter(g) OR allGroups()) BY extractWeek(d), extractYear(d);
costSumBalanceA (LegalEntity le, INTEGER w, INTEGER y) = GROUP SUM costSumBalanceA(Stock s, le, SkuGroup g, DATE d) IF access(s) AND (filter(g) OR allGroups()) BY extractWeek(d), extractYear(d);

