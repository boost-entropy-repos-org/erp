markupSumSold (LegalEntity le, DATE d) = GROUP SUM markupSumSold(Stock s, le, SkuGroup g, d) IF access(s) AND (filter(g) OR allGroups());
sumSold (LegalEntity le, DATE d) = GROUP SUM sumSold(Stock s, le, SkuGroup g, d) IF access(s) AND (filter(g) OR allGroups());
costSumBalanceA (LegalEntity le, DATE d) = GROUP SUM costSumBalanceA(Stock s, le, SkuGroup g, d) IF access(s) AND (filter(g) OR allGroups());

