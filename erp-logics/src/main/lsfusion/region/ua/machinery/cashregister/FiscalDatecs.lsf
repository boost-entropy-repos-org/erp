MODULE FiscalDatecs;

REQUIRE System,
        POS;

loadDefaultCashRegisterModels() + {  EXEC loadDefaultCashRegisterModel('Фискальный регистратор DateCS', 'DateCS', NULL, NULL, NULL, NULL); };

fiscalDatecsPrint 'Напечатать фискальный чек'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsPrintReceiptActionProperty' (Receipt);
postPrint(Receipt receipt) + {  IF sidCashRegisterModel(receipt) == 'DateCS' THEN fiscalDatecsPrint(receipt); }

fiscalDatecsAdvancePaper 'Прогон ленты'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsAdvancePaperActionProperty' ();
fiscalAdvancePaper() + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsAdvancePaper(); }

fiscalDatecsCutReceipt 'Отрезать чек'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsCutReceiptActionProperty' ();
fiscalCutReceipt() + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsCutReceipt(); }

fiscalDatecsXReport 'X-отчёт'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsXReportActionProperty' ();
fiscalXReport() + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsXReport(); }

fiscalDatecsZReport 'Z-отчёт'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsZReportActionProperty' ();
fiscalZReport() + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsZReport(); }

fiscalDatecsUpdateData 'Загрузить информацию'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsUpdateDataActionProperty' ();
fiscalUpdateData() + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsUpdateData(); }

fiscalDatecsService 'Перемещение денег'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsServiceInOutActionProperty' (CashOperation);
fiscalService(CashOperation cashOperation) + {  IF sidCashRegisterModel(cashOperation) == 'DateCS' THEN fiscalDatecsService(cashOperation); }

fiscalDatecsDisplayText 'Вывести текст на дисплей'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsDisplayTextActionProperty' (ReceiptDetail);
fiscalDisplayText (ReceiptDetail receiptDetail) + {  IF sidCashRegisterModel(receiptDetail) == 'DateCS' THEN fiscalDatecsDisplayText(receiptDetail); }

fiscalDatecsCancel 'Отменить чек'  CUSTOM  'lsfusion.erp.region.ua.machinery.cashregister.fiscaldatecs.FiscalDatecsCancelReceiptActionProperty' (Receipt);
fiscalCancel(Receipt receipt) + {  IF sidModelCurrentCashRegister() == 'DateCS' THEN fiscalDatecsCancel(receipt); }