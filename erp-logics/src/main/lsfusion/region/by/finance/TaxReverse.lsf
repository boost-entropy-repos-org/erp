MODULE TaxReverse;

REQUIRE System, Tax;

NAMESPACE Tax;

reverse 'Реверсивный расчет' = DATA BOOLEAN (Range);

EXTEND FORM tax
    PROPERTIES(ra) reverse BEFORE dateFromCurrentRate(ra)
;

EXTEND FORM range
    PROPERTIES(r) reverse
;

EXTEND FORM rangeDialog
    PROPERTIES(r) READONLY reverse
;

