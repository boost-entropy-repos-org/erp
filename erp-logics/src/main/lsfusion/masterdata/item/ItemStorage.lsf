MODULE ItemStorage;

REQUIRE System, Item;

NAMESPACE Item;

expiryText 'Срок хранения' = DATA  VARISTRING[100] (Item) IN itemExpiry;
conditionsText 'Условия хранения' = DATA  VARISTRING[100] (Item) IN itemExpiry;

EXTEND FORM item PROPERTIES(i) SHOWIF showExpiry(i) expiryText, conditionsText;