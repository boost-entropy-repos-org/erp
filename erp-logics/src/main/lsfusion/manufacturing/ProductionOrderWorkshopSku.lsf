MODULE ProductionOrderWorkshopSku;

REQUIRE ProductionOrderSku, ProductionOrderWorkshop;

NAMESPACE Production;

fillNegativeBalanceWorkshopProductDetail 'Добавить строки с отрицательным остатком' (Order o) {
    fillNegativeBalanceProductDetail(o, skuGroup(workshopStock(o)));
}

EXTEND FORM order PROPERTIES fillNegativeBalanceWorkshopProductDetail(o) DRAW pd TOOLBAR;