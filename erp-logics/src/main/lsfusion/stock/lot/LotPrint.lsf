MODULE LotPrint;

REQUIRE Lot; 

NAMESPACE Lot;

count = DATA LOCAL INTEGER (Lot);
FORM printLot 'Печать марок'
    OBJECTS l = Lot
    PROPERTIES(l) count, id, nameSku, code, gtin, series 
    FILTERS count(l)
;

printLot () {
    IF GROUP SUM 1 IF count(Lot l) THEN
        PRINT printLot;
}