MODULE ExportStockSupermag;
REQUIRE SupermagIntegration, Store,Warehouse; //ExportSupermag

EXTEND FORM warehouse
    PROPERTIES (w) idSupermag
;
DESIGN warehouse{
    headerMainParams{
        MOVE PROPERTY (idSupermag(w));
    }
}
EXTEND FORM departmentStore
    PROPERTIES (d) idSupermag
;
DESIGN departmentStore{
    headerMainParams{
        MOVE PROPERTY (idSupermag(d));
    }
}
