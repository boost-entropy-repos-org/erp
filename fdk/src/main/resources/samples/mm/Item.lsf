MODULE Item;

REQUIRE System;

CLASS Item 'Товар';

nameItem 'Наименование' (item) = DATA STRING[100](Item) IN base;
barcodeItem 'Штрих-код' (item) = DATA STRING[13](Item) IN base;

salePriceItem 'Оптовая цена' (item) = DATA NUMERIC[17,2](Item) IN base;

FORM items 'Товары'
	OBJECTS i=Item
	PROPERTIES(i) nameItem, barcodeItem, salePriceItem, ADDOBJ, DELETESESSION
;