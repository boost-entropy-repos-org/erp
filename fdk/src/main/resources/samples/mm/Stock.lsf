MODULE Stock;

REQUIRE System;

CLASS Stock 'Склад';

nameStock 'Наименование' (stock) = DATA STRING[100](Stock) IN base;
addressStock 'Адрес' (stock) = DATA STRING[150](Stock) IN base;

FORM stocks 'Склады'
	OBJECTS s=Stock
	PROPERTIES(s) nameStock, addressStock, ADDOBJ, DELETESESSION
;